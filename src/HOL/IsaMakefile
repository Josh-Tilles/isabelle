## targets

default: HOL
images: HOL HOL-Real HOL-Hyperreal TLA

#Note: keep targets sorted (except for HOL-Library)
test: \
  HOL-Library \
  HOL-Algebra \
  HOL-Auth \
  HOL-AxClasses \
  HOL-BCV \
      HOL-Real-HahnBanach \
      HOL-Real-ex \
  HOL-Hoare \
  HOL-IMP \
  HOL-IMPP \
  HOL-IOA \
  HOL-Induct \
  HOL-Isar_examples \
  HOL-Lambda \
  HOL-Lattice \
  HOL-Lex \
  HOL-MicroJava \
  HOL-MiniML \
  HOL-Modelcheck \
  HOL-NumberTheory \
  HOL-Prolog \
  HOL-Subst \
      TLA-Buffer \
      TLA-Inc \
      TLA-Memory \
  HOL-UNITY \
  HOL-Unix \
  HOL-W0 \
  HOL-ex
    # ^ this is the sort position

all: test images


## global settings

SRC = $(ISABELLE_HOME)/src
OUT = $(ISABELLE_OUTPUT)
LOG = $(OUT)/log


