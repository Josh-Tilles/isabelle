OUT = $(ISABELLE_OUTPUT)
LOG = $(OUT)/log


## HOL

HOL: Pure $(OUT)/HOL

HOL-Base: Pure $(OUT)/HOL-Base

HOL-Plain: Pure $(OUT)/HOL-Plain

HOL-Main: Pure $(OUT)/HOL-Main

HOL-Proofs: Pure $(OUT)/HOL-Proofs

Pure:
	@cd $(SRC)/Pure; $(ISABELLE_TOOL) make Pure

$(OUT)/Pure: Pure

