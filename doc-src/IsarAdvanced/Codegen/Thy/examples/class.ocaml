module Nat = 
struct

type nat = Suc of nat | Zero_nat;;

end;; (*struct Nat*)

module Codegen = 
struct

type 'a null = {null : 'a};;
let null _A = _A.null;;

let rec head _B = function x :: xs -> x
                  | [] -> null _B;;

let null_option () = ({null = None} : ('b option) null);;

let rec dummy
  = head (null_option ()) [Some (Nat.Suc Nat.Zero_nat); None];;

end;; (*struct Codegen*)
