
## targets

default: Thy
images: 
test: Thy

all: images test


## global settings

SRC = $(ISABELLE_HOME)/src
OUT = $(ISABELLE_OUTPUT)
LOG = $(OUT)/log

USEDIR = $(ISATOOL) usedir -v true -i false -d false -C false -D document


## Thy

Thy: $(LOG)/Pure-Thy.gz

$(LOG)/Pure-Thy.gz: Thy/ROOT.ML ../antiquote_setup.ML Thy/intro.thy \
  Thy/syntax.thy
	@$(USEDIR) Pure Thy


## clean

clean:
	@rm -f $(LOG)/Pure-Thy.gz
